                            android:textStyle="bold"
                            android:layout_marginStart="@dimen/dp_10"
                            app:setNameCodeById="@{recent.localteam_id}"/>
                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/team1SquadRV"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>
                    <!--                team 2 squad -->
                    <LinearLayout
                        android:id="@+id/matchdetailsTeam2SquadLL"
                        android:visibility="visible"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <!--                team2 quad-->
                        <TextView
                            android:id="@+id/team2SquadHeadingTV"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textColor="@color/black"
                            android:gravity="start"
                            android:textSize="30sp"
                            android:layout_marginTop="@dimen/dp_10"
                            android:layout_marginBottom="@dimen/dp_10"
                            android:textStyle="bold"
                            android:layout_marginStart="@dimen/dp_10"
                            app:setNameCodeById="@{recent.visitorteam_id}"/>
                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/team2SquadRV"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>
                </LinearLayout>
                <!--                commentry-->
                <LinearLayout
                    android:id="@+id/commentryLL"
                    android:layout_marginTop="@dimen/dp_10"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/matchDetailsCommentryRV"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
                </LinearLayout>


            </LinearLayout>
        </ScrollView>
        <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
            android:id="@+id/backbFAB"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:focusable="true"
            android:layout_marginBottom="30dp"
            android:backgroundTint="#B3F8B3"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            app:iconTint="@color/black"
            app:icon="@drawable/back"
            android:textColor="@color/t20backfab"
            android:text="@string/back"/>
    </RelativeLayout>

</layout>