<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="info"
            type="com.mdasrafulalam.cricwave.model.fixtureswithruns.Data" />
        <variable
            name="recent"
            type="com.mdasrafulalam.cricwave.model.fixturesfilteringdateincludingruns.Data" />
    </data>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.MatchDetailsFragment">
        <ScrollView
            android:id="@+id/scrollviewTop"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <!--                summery-->
                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_margin="@dimen/dp_10"
                    android:elevation="8dp"
                    app:cardCornerRadius="@dimen/dp_10"
                    app:cardElevation="@dimen/dp_10"
                    android:layout_height="wrap_content">
                    <RelativeLayout
                        android:padding="@dimen/dp_10"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <TextView
                            android:id="@+id/detailsMatchItemStatus"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_10"
                            android:layout_marginStart="@dimen/dp_10"
                            android:text="@{recent.status}"
                            android:textColor="@color/color_trail_text"
                            android:textStyle="bold"
                            android:textSize="@dimen/sp20" />
                        <TextView
                            android:id="@+id/detailsMatchItemMatchInfo"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/detailsMatchItemStatus"
                            android:layout_alignParentStart="true"
                            android:text="@{recent.type}"
                            android:layout_marginStart="@dimen/dp_10"
                            android:textSize="@dimen/sp14"
                            android:layout_marginTop="@dimen/dp_10"
                            tools:text="T20"/>
                        <TextView
                            android:id="@+id/breakpoint"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/detailsMatchItemStatus"
                            android:layout_toEndOf="@+id/detailsMatchItemMatchInfo"
                            android:layout_marginStart="@dimen/dp_10"
                            android:textSize="@dimen/sp14"
                            android:layout_marginTop="@dimen/dp_10"
                            android:text="@string/star"/>
                        <TextView
                            android:id="@+id/detailsMatchVenuTV"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/breakpoint"
                            android:layout_below="@+id/detailsMatchItemStatus"
                            app:setVenuById="@{recent.venue_id}"
                            android:layout_marginStart="@dimen/dp_10"
                            android:textSize="@dimen/sp14"
                            android:maxLines="2"
                            android:layout_marginTop="@dimen/dp_10"
                            tools:text="Shere-Bangla Stadium, Mirpur"/>
                        <ImageView
                            android:id="@+id/detailsTeam1IV"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginTop="@dimen/dp_10"
                            android:layout_marginStart="10dp"
                            tools:src="@drawable/house"
                            app:setImageById="@{recent.localteam_id}"
                            android:layout_below="@+id/detailsMatchVenuTV"
                            tools:ignore="ContentDescription" />
                        <TextView
                            android:id="@+id/detailsTeam1NameTV"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            tools:text="WI"
                            android:layout_toEndOf="@+id/detailsTeam1IV"
                            app:setNameCodeById="@{recent.localteam_id}"
                            android:layout_marginTop="@dimen/dp_10"
                            android:layout_marginStart="@dimen/dp_10"
                            android:textSize="@dimen/sp20"
                            android:layout_below="@+id/detailsMatchVenuTV"
                            />
                        <ImageView
                            android:id="@+id/detailsTeam2IV"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_below="@+id/detailsTeam1IV"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="10dp"
                            app:setImageById="@{recent.visitorteam_id}"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/series" />

                        <TextView
                            android:id="@+id/detailsTeam2NameTV"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/detailsTeam1NameTV"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="10dp"
                            android:layout_toEndOf="@+id/detailsTeam2IV"
                            android:textSize="@dimen/sp20"
                            app:setNameCodeById="@{recent.visitorteam_id}"
                            tools:text="PAK" />

                        <TextView
                            android:id="@+id/detailsTeam1ScoreTV"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/detailsMatchVenuTV"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="10dp"
                            android:layout_alignParentEnd="true"
                            android:textSize="@dimen/sp20"
                            tools:text="180/9 (20 Overs)" />
                        <TextView
                            android:id="@+id/detailsTeam2ScoreTV"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/detailsTeam1ScoreTV"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="10dp"
                            android:layout_alignParentEnd="true"
                            android:textSize="@dimen/sp20"
                            tools:text="181/9 (20 Overs)" />
                        <TextView
                            android:id="@+id/liveMatchResultOrStartTV"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            tools:text="Pakistan Won by 4 wickets"
                            android:layout_alignParentStart="true"
                            android:layout_marginTop="@dimen/dp_10"
                            android:text="@{recent.note}"
                            android:layout_marginStart="@dimen/dp_10"
                            android:textSize="@dimen/sp20"
                            android:layout_below="@+id/detailsTeam2IV"/>
                        <TextView
                            android:id="@+id/liveMatchMOTMTV"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/motm"
                            android:layout_alignParentStart="true"
                            android:layout_marginTop="@dimen/dp_10"
                            android:textColor="@color/black"
                            android:layout_marginStart="@dimen/dp_10"
                            android:textSize="@dimen/sp20"
                            android:layout_below="@+id/liveMatchResultOrStartTV"/>
                        <TextView
                            android:id="@+id/liveMatchMOTMPlayerTV"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            tools:text="Mushfiqur Rahim"
                            android:textColor="@color/black"
                            android:layout_marginTop="@dimen/dp_10"
                            android:layout_toEndOf="@+id/liveMatchMOTMTV"
                            app:setPlayerNameById="@{recent.man_of_match_id}"
                            android:layout_marginStart="@dimen/dp_10"
                            android:textSize="@dimen/sp20"
                            android:layout_below="@+id/liveMatchResultOrStartTV"/>
                        <TextView
                            android:id="@+id/firstUP"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dp_10"
                            android:text="@string/first_umpire"
                            android:layout_below="@+id/liveMatchMOTMPlayerTV"
                            android:textSize="@dimen/sp16"
                            android:textColor="@color/black"/>
                        <TextView
                            android:id="@+id/matchdetails1stUmpInfo"
                            android:layout_toEndOf="@+id/firstUP"
                            android:layout_marginStart="@dimen/dp_10"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/liveMatchMOTMPlayerTV"
                            tools:text="Tony"
                            android:textColor="@color/black"
                            android:textSize="@dimen/sp16" />
                        <TextView
                            android:id="@+id/secondUP"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dp_10"
                            android:text="@string/second_umpire"
                            android:layout_below="@+id/matchdetails1stUmpInfo"
                            android:textSize="@dimen/sp16"
                            android:textColor="@color/black"/>
                        <TextView
                            android:id="@+id/matchdetails2ndUmpInfo"
                            android:layout_toEndOf="@+id/secondUP"
                            android:layout_marginStart="@dimen/dp_10"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/matchdetails1stUmpInfo"
                            tools:text="Tony"
                            android:textColor="@color/black"
                            android:textSize="@dimen/sp16" />
                        <TextView
                            android:id="@+id/tvUP"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dp_10"
                            android:text="@string/tv_umpire"
                            android:layout_below="@+id/matchdetails2ndUmpInfo"
                            android:textSize="@dimen/sp16"
                            android:textColor="@color/black"/>
                        <TextView
                            android:id="@+id/matchdetailsTvUmpInfo"
                            android:layout_toEndOf="@+id/tvUP"
                            android:layout_marginStart="@dimen/dp_10"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/matchdetails2ndUmpInfo"
                            tools:text="Tony"
                            android:textColor="@color/black"
                            android:textSize="@dimen/sp16" />
                    </RelativeLayout>
                </androidx.cardview.widget.CardView>
                <LinearLayout
